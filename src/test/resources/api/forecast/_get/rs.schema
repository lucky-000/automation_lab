              "day": {
                "type": "number"
              },
              "night": {
                "type": "number"
              },
              "eve": {
                "type": "number"
              },
              "morn": {
                "type": "number"
              }
            },
            "additionalProperties": true,
            "required": [
              "day",
              "night",
              "eve",
              "morn"
            ]
          },
          "pressure": {
            "type": "integer"
          },
          "humidity": {
            "type": "integer"
          },
          "dew_point": {
            "type": "number"
          },
          "wind_speed": {
            "type": "number"
          },
          "wind_deg": {
            "type": "integer"
          },
          "weather": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "id": {
                  "type": "integer"
                },
                "main": {
                  "type": "string"
                },
                "description": {
                  "type": "string"
                },
                "icon": {
                  "type": "string"
                }
              },
              "additionalProperties": true,
              "required": [
                "id",
                "main",
                "description",
                "icon"
              ]
            },
            "additionalItems": true
          },
          "clouds": {
            "type": "integer"
          },
          "pop": {
            "type": [
              "integer",
              "number"
            ]
          },
          "snow": {
            "type": "number"
          },
          "uvi": {
            "type": [
              "number",
              "integer"
            ]
          },
          "rain": {
            "type": "number"
          }
        },
        "additionalProperties": true,
        "required": [
          "dt",
          "sunrise",
          "sunset",
          "temp",
          "feels_like",
          "pressure",
          "humidity",
          "dew_point",
          "wind_speed",
          "wind_deg",
          "weather",
          "clouds",
          "pop",
          "uvi"
        ]
      },
